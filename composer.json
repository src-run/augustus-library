{
  "name"        : "src-run/augustus-library",
  "license"     : "MIT",
  "type"        : "metapackage",
  "homepage"    : "https://src.run/augustus-library",
  "description" : "composer meta-package to require all augustus-* packages",
  "support" : {
    "email"  : "rmf@src.run",
    "issues" : "https://src.run/augustus-library/issues",
    "source" : "https://src.run/augustus-library/git"
  },
  "keywords"   : [
    "augustus-library", "augustus", "library", "rmf", "src-run"
  ],
  "authors"    : [
    {
      "name"    : "Rob Frawley 2nd",
      "email"   : "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role"    : "Project Lead"
    }
  ],
  "require" : {
    "php"                                  : "^5.6||^7.0",
    "src-run/augustus-polyfill-library"    : "self.version",
    "src-run/augustus-utility-library"     : "self.version",
    "src-run/augustus-primitive-library"   : "self.version",
    "src-run/augustus-reflection-library"  : "self.version",
    "src-run/augustus-exception-library"   : "self.version",
    "src-run/augustus-deprecation-library" : "self.version"
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config": {
    "bin-dir" : "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master" : "0.6-dev"
    }
  }
}
